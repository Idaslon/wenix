{
  "root": true,
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "custom",
    "plugin:@typescript-eslint/recommended"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-useless-constructor": "off",
    "@typescript-eslint/member-delimiter-style": "off"
  }
}
